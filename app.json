{
  "expo": {
    "name": "spotify-app",
    "slug": "spotify-app",
    "version": "1.0.0",
    "scheme": "spotify-app",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
