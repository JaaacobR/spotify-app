        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "d3a5ece2-c921-4d8c-b138-0f2ce7de9f4a"
      }
    },
    "owner": "jryszka"
  }
